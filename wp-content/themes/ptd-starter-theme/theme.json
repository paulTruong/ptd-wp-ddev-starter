{
	"$schema": "https://schemas.wp.org/wp/6.5/theme.json",
	"settings": {
		"custom": {
			"spacing": {
				"1": "4px",
				"2": "8px",
				"3": "12px",
				"4": "16px",
				"5": "24px",
				"6": "32px",
				"7": "48px",
				"8": "64px",
				"9": "96px",
				"10": "128px",
				"11": "192px",
				"12": "256px",
				"13": "384px",
				"14": "512px",
				"15": "640px",
				"16": "768px"
			},
			"font-sizes": {
				"1": "0.75rem",
				"2": "0.875rem",
				"3": "1rem",
				"4": "1.125rem",
				"5": "1.25rem",
				"6": "1.5rem",
				"7": "1.875rem",
				"8": "2.25rem",
				"9": "3rem",
				"10": "3.75rem",
				"11": "4.5rem"
			},
			"typography": 
			{
				"body-font-size": "clamp(0.875rem, 0.787rem + 0.3756vi, 1.125rem)"
			}
		},
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#242424",
					"name": "Foreground",
					"slug": "foreground"
				},
				{
					"color": "#ffffff",
					"name": "Background",
					"slug": "background"
				},
				{
					"color": "#FCC934",
					"name": "Yellow",
					"slug": "yellow"
				},
				{
					"color": "#FE5E23",
					"name": "Orange",
					"slug": "orange"
				},
				{
					"color": "#FB202F",
					"name": "Bright Red",
					"slug": "bright-red"
				},
				{
					"color": "#B61D3C",
					"name": "Dark Red",
					"slug": "dark-red"
				},
				{
					"color": "#6C2240",
					"name": "Maroon",
					"slug": "maroon"
				}
			]
		},
		"layout": {
			"contentSize": "46.25rem",
			"wideSize": "75rem"
		},
		"spacing": {
			"spacingSizes": [
				{
					"name": "8px (0.5rem)",
					"size": "0.5rem",
					"slug": "10"
				},
				{
					"name": "16px (1rem)",
					"size": "1rem",
					"slug": "20"
				},
				{
					"name": "32px (2rem)",
					"size": "2rem",
					"slug": "30"
				},
				{
					"name": "48px (3rem) - Fluid",
					"size": "clamp(2.00rem, calc(1.71rem + 1.43vw), 3.00rem)",
					"slug": "40"
				},
				{
					"name": "64px (4rem) - Fluid",
					"size": "clamp(2.00rem, calc(1.43rem + 2.86vw), 4.00rem)",
					"slug": "50"
				},
				{
					"name": "96px (6rem) - Fluid",
					"size": "clamp(4.00rem, calc(3.43rem + 2.86vw), 6.00rem)",
					"slug": "60"
				},
				{
					"name": "192px (12rem) - Fluid",
					"size": "clamp(3.00rem, calc(0.43rem + 12.86vw), 12.00rem)",
					"slug": "70"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Noto Sans",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/NotoSans-VariableFont_wdth,wght.ttf"
							]
						},
						{
							"fontFamily": "Noto Sans",
							"fontStyle": "italic",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/NotoSans-Italic-VariableFont_wdth,wght.ttf"
							]
						}
					],
					"fontFamily": "'Noto Sans', sans-serif",
					"name": "Noto Sans",
					"slug": "noto-sans"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Noto Sans Display",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/NotoSansDisplay-VariableFont_wdth,wght.ttf"
							]
						},
						{
							"fontFamily": "Noto Sans Display",
							"fontStyle": "italic",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/NotoSansDisplay-Italic-VariableFont_wdth,wght.ttf"
							]
						}
					],
					"fontFamily": "'Noto Sans', sans-serif",
					"name": "Noto Sans Display",
					"slug": "noto-sans-display"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Noto Serif",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/NotoSerif-VariableFont_wdth,wght.ttf"
							]
						},
						{
							"fontFamily": "Noto Serif",
							"fontStyle": "italic",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/NotoSerif-Italic-VariableFont_wdth,wght.ttf"
							]
						}
					],
					"fontFamily": "'Noto Serif', serif",
					"name": "Noto Serif",
					"slug": "noto-serif"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Noto Serif Display",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/NotoSerifDisplay-VariableFont_wdth,wght.ttf"
							]
						},
						{
							"fontFamily": "Noto Serif Display",
							"fontStyle": "italic",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/NotoSerifDisplay-Italic-VariableFont_wdth,wght.ttf"
							]
						}
					],
					"fontFamily": "'Noto Serif Display', serif",
					"name": "Noto Serif Display",
					"slug": "noto-serif"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "clamp(0.88rem, calc(0.83rem + 0.23vw), 1.00rem)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(1.00rem, calc(0.91rem + 0.45vw), 1.25rem)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(1.25rem, calc(1.02rem + 1.14vw), 1.88rem)",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "clamp(1.50rem, calc(0.95rem + 2.73vw), 3.00rem)",
					"slug": "xlarge"
				},
				{
					"name": "2X Large",
					"size": "clamp(1.88rem, calc(0.92rem + 4.77vw), 4.50rem)",
					"slug": "xxlarge"
				},
				{
					"name": "Huge",
					"size": "clamp(2.25rem, calc(0.80rem + 7.27vw), 6.25rem)",
					"slug": "huge"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "8px"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--30)",
						"left": "var(--wp--preset--spacing--50)",
						"right": "var(--wp--preset--spacing--50)",
						"top": "var(--wp--preset--spacing--30)"
					}
				},
				"typography": {
					"letterSpacing": "0.5px"
				}
			},
			"core/image": {
				"border": {
					"radius": "8px"
				}
			},
			"core/media-text": {
				"spacing": {
					"padding": {
						"left": "0",
						"right": "0"
					}
				}
			},
			"core/navigation": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--noto-sans)",
					"letterSpacing": "0.5px",
					"textDecoration": "none"
				}
			},
			"core/quote": {
				"border": {
					"radius": "8px"
				},
				"color": {
					"background": "#FCC934",
					"text": "var(--wp--preset--color--foreground)"
				},
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--40)",
						"left": "0",
						"right": "0",
						"top": "var(--wp--preset--spacing--40)"
					},
					"padding": {
						"bottom": "var(--wp--preset--spacing--10)",
						"left": "var(--wp--preset--spacing--30)",
						"right": "var(--wp--preset--spacing--30)",
						"top": "var(--wp--preset--spacing--10)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontStyle": "italic"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--noto-sans)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"letterSpacing": "0.5px",
					"textDecoration": "none"
				}
			}
		},
		"color": {
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"button": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--noto-sans)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--noto-serif)",
					"fontSize": "var(--wp--preset--font-size--xxlarge)",
					"lineHeight": "1.1"
				}
			},
			"h2": {
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--50)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--noto-serif)",
					"fontSize": "var(--wp--preset--font-size--xlarge)",
					"fontStyle": "normal",
					"fontWeight": "400",
					"lineHeight": "1.1"
				}
			},
			"h3": {
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--30)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--noto-serif)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontStyle": "normal",
					"fontWeight": "400"
				}
			},
			"h4": {
				"typography": {
					"fontStyle": "normal",
					"fontWeight": "400"
				}
			},
			"h5": {
				"typography": {
					"fontStyle": "normal",
					"fontWeight": "400"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--noto-serif)"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--orange)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"typography": {
					"textDecoration": "underline"
				}
			}
		},
		"spacing": {
			"padding": {
				"left": "var(--wp--preset--spacing--20)",
				"right": "var(--wp--preset--spacing--20)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--noto-sans)",
			"fontSize": "var(--wp--custom--typography--body-font-size)",
			"lineHeight": "1.4"
		}
	},
	"version": 2
}